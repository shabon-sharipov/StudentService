{
  "ApplicationName": "elastic-search-app",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticSearchConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "RabbitMQ": {
    "Url": "amqp://guest:guest@localhost:5672",
    "ExchangeName": "DemoExchange",
    "RoutinKey": "demo-routin-key",
    "QueueName": "DemoQueue"
  },
  "MongoDb": {
    "ConectionString": "mongodb://localhost:27017/",
    "DatabaseName": "MongoDb"
  },
  "AllowedHosts": "*"
}